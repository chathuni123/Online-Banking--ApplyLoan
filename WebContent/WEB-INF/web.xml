<?xml version="1.0" encoding="UTF-8"?>

<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee
         http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
         version="3.1">
         
         
   <servlet>
     <servlet-name>apply</servlet-name>
     <servlet-class>com.Applicant.ApplyServlet</servlet-class>
   </servlet>
   
     <servlet-mapping>
     <servlet-name>apply</servlet-name>
     <url-pattern>/log</url-pattern>
   </servlet-mapping>
         
   <servlet>
     <servlet-name>InsertApplicant</servlet-name>
     <servlet-class>com.Applicant.ApplicantInsert</servlet-class>
   </servlet>
         
   <servlet-mapping>
     <servlet-name>InsertApplicant</servlet-name>
     <url-pattern>/insert</url-pattern>
   </servlet-mapping>
   
   <servlet> 
     <servlet-name>UpdateApplicant</servlet-name>
     <servlet-class>com.Applicant.UpdateApplicantServlet</servlet-class>
   </servlet>
   <servlet-mapping>
     <servlet-name>UpdateApplicant</servlet-name>
     <url-pattern>/update</url-pattern>
   </servlet-mapping>
   
   <servlet> 
      <servlet-name>DeleteApplicant</servlet-name>
      <servlet-class>com.Applicant.DeleteApplicantServlet</servlet-class>
   </servlet>
   <servlet-mapping>
       <servlet-name>DeleteApplicant</servlet-name>
       <url-pattern>/delete</url-pattern>
   </servlet-mapping>
         
</web-app>